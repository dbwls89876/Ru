package example;

public class Cylinder extends Circle{
	
	

	public Cylinder(int r, int h) {
		height = h;
		super(r);
	}

	public double getVolume() {
		return getArea()*height;
	}
	
	@Override
	public void showInfo() {
		System.out.println("실린더의 높이 : " + height);
		System.out.println("실린더의 부피 : " + getVolume());
	}
}
