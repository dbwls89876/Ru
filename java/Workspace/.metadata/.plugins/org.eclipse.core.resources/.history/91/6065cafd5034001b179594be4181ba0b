package example;

public class ShapeTest {

	public static void main(String[] args) {
		Rectangle r = new Rectangle(3, 5, 3, 4);
		Triangle t = new Triangle(2, 4);
		Circle c = new Circle(3);
		Cylinder cy = new Cylinder(3, 5);
	
		r.showInfo();
		t.showInfo();
		c.showInfo();
		cy.showInfo();
	}

}
